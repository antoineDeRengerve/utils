	excludesfile = /home/antoroll/.gitignore_global
[alias]
	amend = commit --amend
	st = status
	oneline = log --pretty=oneline --abbrev-commit --graph --decorate
	last = log -1 HEAD
	co = checkout
	ci = commit
	br = branch
	brv = branch -vv
	st = status
    	stt = diff --name-status
	dic = diff --cached	
	diffstat = diff --stat
	unstage = reset HEAD –
	diffmeld = difftool –tool=mel

